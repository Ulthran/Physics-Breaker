import Foundation


class DataManagement{
    var numIDKs: Int = 5
    
    init(){
    }
    
    func getNumIDKs() ->Int{
        return numIDKs
    }
    
    func addNumIDKs(addNum: Int){
        numIDKs += addNum
    }
}
