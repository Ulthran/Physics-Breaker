import Foundation
import UIKit
import SpriteKit


class StringX {
    var len: Double
    var ten: Double
    
    init(){
        len = 0
        ten = 0
    }
    
    func getLen() ->Double{
        return len
    }
    
    func setLen(lenX: Double){
        len = lenX
    }
    
    func getTen() ->Double{
        return ten
    }
    
    func setTen(tenX: Double){
        ten = tenX
    }
}























