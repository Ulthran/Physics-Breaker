import Foundation
import  UIKit
import SpriteKit


class FrictionlessMass {
    /*All the field declarations*/
    private var mass: Double
    private var acc: Double
    private var vel: Double
    private var elast: Bool
    
    init(){
        mass = 0
        acc = 0
        vel = 0
        elast = true
    }
    
    func getMass() ->Double{
        return mass
    }
    
    func setMass(massX: Double){
        mass = massX
    }
    
    func getAcc() ->Double{
        return acc
    }
    
    func setAcc(accX: Double){
        acc = accX
    }
    
    func getVel() ->Double{
        return vel
    }
    
    func setVel(velX: Double){
        vel = velX
    }
    
    func getElast() ->Bool{
        return elast
    }
    
    func setElast(elastX: Bool){
        elast = elastX
    }
}


























