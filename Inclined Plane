import Foundation
import UIKit
import SpriteKit


class InclinedPlane {
    private var u: Double
    private var len: Double
    private var ang: Double
    
    init(){
        u = 0
        len = 0
        ang = 0
    }
    
    func getU() ->Double{
        return u
    }
    
    func setU(uX: Double){
        u = uX
    }
    
    func getLen() ->Double{
        return len
    }
    
    func setLen(lenX: Double){
        len = lenX
    }
    
    func getAng() ->Double{
        return ang
    }
    
    func setAng(angX: Double){
        ang = angX
    }
}





















