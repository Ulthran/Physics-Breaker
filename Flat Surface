import Foundation
import UIKit
import SpriteKit


class FlatSurface {
    private var u: Double
    private var len: Double
    
    init(){
        u = 0
        len = 0
    }
    
    func getU() ->Double{
        return u
    }
    
    func setU(uX: Double){
        u = uX
    }
    
    func getLen() ->Double{
        return len
    }
    
    func setLen(lenX: Double){
        len = lenX
    }
}
























