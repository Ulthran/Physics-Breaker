import Foundation
import UIKit
import SpriteKit


class Cliff {
    private var u: Double
    private var lenUpper: Double
    private var lenLower: Double
    private var height: Double
    private var ang: Double
    
    init(){
        u = 0
        lenUpper = 0
        lenLower = 0
        height = 0
        ang = 0
    }
    
    func getU() ->Double{
        return u
    }
    
    func setU(uX: Double){
        u = uX
    }
    
    func getLenUpper() ->Double{
        return lenUpper
    }
    
    func setLenUpper(lenUpperX: Double){
        lenUpper = lenUpperX
    }
    
    func getLenLower() ->Double{
        return lenLower
    }
    
    func setLenLower(lenLowerX: Double){
        lenLower = lenLowerX
    }
    
    func getHeight() ->Double{
        return height
    }
    
    func setHeight(heightX: Double){
        height = heightX
    }
    
    func getAng() ->Double{
        return ang
    }
    
    func setAng(angX: Double){
        ang = angX
    }
}

























