import Foundation
import UIKit
import SpriteKit


class Wall {
    var height: Double
    var width: Double
    
    init(){
        height = 0
        width = 0
    }
    
    func getHeight() ->Double{
        return height
    }
    
    func setHeight(heightX: Double){
        height = heightX
    }
    
    func getWidth() ->Double{
        return width
    }
    
    func setWidth(widthX: Double){
        width = widthX
    }
}























