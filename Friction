import Foundation
import UIKit
import SpriteKit


class Friction {
    var force: Double
    var ang: Double
    
    init(){
        force = 0
        ang = 0
    }
    
    func getForce() ->Double{
        return force
    }
    
    func setForce(forceX: Double){
        force = forceX
    }
    
    func getAng() ->Double{
        return ang
    }
    
    func setAng(angX: Double){
        ang = angX
    }
}























